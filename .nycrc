{
  "cache": false,
  "check-coverage": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "test/**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true,
  "branches": 90,
  "lines": 90,
  "functions": 90,
  "statements": 90,
  "per-file": true
}